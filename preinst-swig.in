#!/bin/sh
builddir=`dirname $0`
srcdir=`cd "$builddir" && cd '@srcdir@' && pwd`
SWIG_LIB=$srcdir/Lib
# For native Windows version of SWIG
"$builddir/swig" -version 2>&1 | \
  grep -i 'compiled with' | grep -i -e ' cl ' -e ' cl.exe ' && SWIG_LIB=`cygpath -w $srcdir/Lib`
export SWIG_LIB
exec "$builddir/swig" $*
